<p id="notice"><%= notice %></p>

<h1>Article1s</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @article1s.each do |article1| %>
      <tr>
        <td><%= article1.title %></td>
        <td><%= article1.description %></td>
        <td><%= link_to 'Show', article1 %></td>
        <td><%= link_to 'Edit', edit_article1_path(article1) %></td>
        <td><%= link_to 'Destroy', article1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article1', new_article1_path %>
